  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/cypress": "^1.1.6",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.3",
    "cypress-cdp": "^1.6.82",
    "cypress-image-diff-html-report": "^2.2.0",
    "cypress-image-diff-js": "^2.4.0",
    "jsonc-eslint-parser": "^2.4.0",
    "log4brains": "^1.1.0",
    "prettier": "^3.6.2",
    "react-devtools": "^6.1.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.5"
