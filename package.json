{
  "name": "grid-assessment",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "adr/preview": "log4brains preview",
    "adr/new": "log4brains adr new",
    "start": "vite dev",
    "test/component": "concurrently 'cypress open --component --browser edge' 'npm react-devtools'",
    "test/component/ci": "cypress run --component --browser edge",
